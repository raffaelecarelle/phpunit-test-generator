{
    "name": "raffaelecarelle/phpunit-test-generator",
    "description": "Generate PHPUnit test classes for a PHP class.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan H. Wage",
            "email": "jonwage@gmail.com"
        },
        {
            "name": "Raffaele Carelle",
            "email": "raffaele.carelle@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "doctrine/inflector": "2.1.x-dev",
        "nikic/php-parser": "^v5.3.1",
        "symfony/console": "^v7.1.5",
        "symfony/filesystem": "^v7.1.5"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0.0",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^1.12.7",
        "phpstan/phpstan-deprecation-rules": "^1.2.1",
        "phpstan/phpstan-phpunit": "^1.4.0",
        "phpstan/phpstan-strict-rules": "^1.6.1",
        "phpunit/phpunit": "^11.4.2",
        "rector/rector": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PHPUnitTestGenerator\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnitTestGenerator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "bin": [
        "bin/generate-unit-test"
    ]
}
